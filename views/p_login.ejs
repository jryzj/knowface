<div class="row my-5">
    <div class="col-md-4"></div>
    <div class="col-md-4">
        <form id="p_login_form_login">
            <div class="form-group">
                <label for="username">用户名</label>
                <input type="text" class="form-control" id="p_login_input_username" name="p_login_input_username" placeholder="请输入用户名">
            </div>
            <div class="form-group">
                <label for="password">密码</label>
                <input type="password" class="form-control" id="p_login_input_password" name="p_login_input_password" placeholder="请输入密码">
            </div>
            <button type="button" class="btn btn-outline-secondary my-3" id="p_login_btn_submit">登陆</button>
        </form>
        <div class="alert alert-secondary" id="p_login_div_feedback">
            请输入用户名、密码。
        </div>

    </div>
    <div class="col-md-4"></div>
</div>