<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
        <% if (result) { %>
        <div class="jumbotron jumbotron-fluid">
            <h1 class="display-4 d-flex justify-content-center">您已经注册成功！</h1>
            <p class="lead d-flex justify-content-center"><a href="/user/p_userdocs">请点击进入您的管理后台</a>或<a href="/">返回主页</a></p>
        </div>
        <% } else { %>
        <div class="jumbotron jumbotron-fluid">
            <h1 class="display-4 d-flex justify-content-center">注册失败！</h1>
            <p class="lead d-flex justify-content-center"><a href="/p_signup">请重新注册</a>或<a href="/">返回主页</a></p>
        </div>

        <% } %>
    </div>
    <div class="col-md-3"></div>
</div>